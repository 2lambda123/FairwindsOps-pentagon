# Variables set here are not necessarily available as environment variables in your shell.
# To add any, modify the appropriate list in env-vars.sh

VPC_NAME: "{{ vpc_name }}"

AWS_ACCESS_KEY: "{{ aws_access_key }}"
TF_VAR_aws_access_key: "{{ aws_access_key }}"
AWS_DEFAULT_REGION:  "{{ aws_default_region }}"
AWS_AVAILABILITY_ZONES: "{{ aws_availability_zones }}"
AWS_AVAILABILITY_ZONE_COUNT: "{{ aws_availability_zone_count }}"

ANSIBLE_CONFIG: '$INFRASTRUCTURE_REPO/config/local/ansible.cfg'
KUBECONFIG: '$INFRASTRUCTURE_REPO/config/private/kube_config'
AWS_INVENTORY_PATH: '$INFRASTRUCTURE_REPO/plugins/'

INFRASTRUCTURE_BUCKET: "{{ infrastructure_bucket }}"
